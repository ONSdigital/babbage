{
  "name": "babbage-web",
  "version": "0.0.1",
  "description": "Web front-end of ONS Website Babbage",
  "main": " ",
  "scripts": {
    "clean": "rimraf dist",
    "copy": "mkdir dist && copyfiles app/**/*.html dist && cp *.html dist && cp *.md dist && cp *.txt dist",
    "build": "npm run clean && npm run copy"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ONSdigital/babbage.git"
  },
  "keywords": [
    "ons",
    "statistics",
    "web",
    "babbage"
  ],
  "author": "Bren",
  "license": "Open Government License v2.0",
  "bugs": {
    "url": "https://github.com/ONSdigital/babbage/issues"
  },
  "dependencies": {
    "ONS-Pattern-Library": "git://github.com/ONSdigital/pattern-library"
  },
  "devDependencies": {
    "rimraf": "latest",
    "cat": "latest",
    "cpy": "~3.1.0",
    "copyfiles": "~0.2.1"
  }
}
