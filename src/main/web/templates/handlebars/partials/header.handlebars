			{{!-- SEARCH BAR --}}
			<div class="search nav-search--hidden" id="searchBar">
				<div class="wrapper">
					{{!-- <div class=""> --}}
						<form class="col-wrap search__form" id="searchBar" action="/search">
							<label class="search__label col col--md-22 col--lg-24" for="nav-search">Search for keyword(s) or time series ID</label>
							<input type="search" autocomplete="off" class="search__input col col--md-18 col--lg-27" id="nav-search" name="q" value="{{parameters.q.0}}">
							<button type="submit" class="search__button col--md-7 col--lg-8" id="nav-search-submit">
								<span>{{labels.search}}</span>
								<span class="icon--search"></span>
							</button>
						</form>
					{{!-- </div> --}}
				</div>
			</div>
