"@type": "Dataset",
{{> partials/json-ld/author }}
"datePublished": "{{description.releaseDate}}",
"dateModified": "{{description.releaseDate}}",
"discussionURL": "https://{{location.hostname}}{{relatedDocuments.0.uri}}",
"keywords": ["{{description.cdid}}", "{{description.datasetId}}"],
{{> partials/json-ld/publisher }}
"image": "https://{{location.hostname}}{{uri}}/linechartimage",  
"distribution":[
    {
        "@type":"DataDownload",
        "encodingFormat":"XLS",
        "contentUrl": "https://{{location.hostname}}/generator?format=xls&uri={{uri}}"
    },
    {
        "@type":"DataDownload",
        "encodingFormat":"CSV",
        "contentUrl": "https://{{location.hostname}}/generator?format=csv&uri={{uri}}"
    }
],