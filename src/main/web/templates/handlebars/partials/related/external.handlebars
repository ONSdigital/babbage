{{#if links}}
	<div class="tiles__item tiles__item--nav-type flush-col">
		<h3 class="tiles__title-h3 tiles__title-h3--nav">
			Related external links
		</h3>
		<div class="tiles__content--nav">
			<ul class="list--neutral">
				{{#each links}}
					<li><a href="{{uri}}">{{title}}</a></li>
				{{/each}}
			</ul>
		</div>
	</div>
{{/if}}