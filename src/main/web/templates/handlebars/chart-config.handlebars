	{{#if_eq chartType "area"}}{{> highcharts/config/line-chart-config area=true}}{{/if_eq}}
	{{#if_eq chartType "barline"}}{{> highcharts/config/barline-chart-config}}{{/if_eq}}
	{{#if_eq chartType "rotated-barline"}}{{> highcharts/config/barline-chart-config rotated=true}}{{/if_eq}}
	{{#if_eq chartType "rotated"}}{{> highcharts/config/rotated-bar-chart-config}}{{/if_eq}}
	{{#if_eq chartType "bar"}}{{> highcharts/config/bar-chart-config}}{{/if_eq}}
	{{#if_eq chartType "scatter"}}{{> highcharts/config/scatter-chart-config}}{{/if_eq}}
	{{#if_eq chartType "pie"}}{{> highcharts/config/pie-chart-config}}{{/if_eq}}
	{{#if_eq chartType "population"}}{{> highcharts/config/population-chart-config}}{{/if_eq}}
