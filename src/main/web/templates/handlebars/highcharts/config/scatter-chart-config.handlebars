{{#partial "chart-config"}}
options.xAxis.categories=[]; {{!-- override categories --}}

options.series=[{
	    	name: "{{{headers.1}}}",
	    	type: "errorbar",
	    	data: [{{#each data}}[{{alt (get this headers.1) "null"}},{{alt (get this headers.2) "null"}}],{{/each}}]
	    }, {
			name: "{{{headers.3}}}",
			type: "scatter",
			data: [{{#each data}}{{alt (get this headers.3) "null"}},{{/each}}]
	    }];

options.series=[{{#each series}}{
		name: "{{{this}}}",
		type: "scatter",
		data: [{{#each data}}{{alt (get this ..) "null"}},{{/each}}]
	},{{/each}}];
{{/partial}}

{{!-- This denotes the template inheritcs from base-chart-config--}}
{{> highcharts/config/base-chart-config}} 