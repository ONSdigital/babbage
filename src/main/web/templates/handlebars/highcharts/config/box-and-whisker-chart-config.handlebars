{{#partial "chart-config"}}
options.chart.type="boxplot";
options.series= [{
	    	name: "{{{headers.0}}}",
	    	data: [{{#each data}}[{{alt (get this headers.1) "null"}},{{alt (get this headers.2) "null"}},{{alt (get this headers.3) "null"}},{{alt (get this headers.4) "null"}},{{alt (get this headers.5) "null"}}],{{/each}}]
	    }];

options.tooltip={
	    formatter: function() {
	    	console.log(this);
	    	return this.x + '<br/>' + '<b>{{{headers.0}}}</b><br/>' +
	    		'{{headers.1}}: ' + Highcharts.numberFormat(this.points[0].point.low, {{alt decimalPlaces 0}}) + '<br/>' +
	    		'{{headers.2}}: ' + Highcharts.numberFormat(this.points[0].point.q1, {{alt decimalPlaces 0}}) + '<br/>' +
	    		'{{headers.3}}: ' + Highcharts.numberFormat(this.points[0].point.median, {{alt decimalPlaces 0}}) + '<br/>' +
	    		'{{headers.4}}: ' + Highcharts.numberFormat(this.points[0].point.q3, {{alt decimalPlaces 0}}) + '<br/>' +
	    		'{{headers.5}}: ' + Highcharts.numberFormat(this.points[0].point.high, {{alt decimalPlaces 0}});
	    },
		shared: true
	};

{{/partial}}
{{!-- This denotes the template inherits from base-chart-config--}}
{{> highcharts/config/base-chart-config}} 