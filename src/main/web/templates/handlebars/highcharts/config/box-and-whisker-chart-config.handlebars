{{#partial "chart-config"}}
options.chart.type="boxplot";
options.series= [{
	    	name: "{{{sub (sup headers.0)}}}",
	    	data: [{{#each data}}[{{num (lookup this headers.1)}},{{num (lookup this headers.2)}},{{num (lookup this headers.3)}},{{num (lookup this headers.4)}},{{num (lookup this headers.5)}}],{{/each}}]
	    }];

options.tooltip.formatter=function() {
	    	return this.x + '<br/>' + '<b>{{{sub (sup headers.0)}}}</b><br/>' +
	    		'{{{sub (sup headers.1)}}}: ' + Highcharts.numberFormat(this.points[0].point.low, {{num decimalPlaces}}) + '<br/>' +
	    		'{{{sub (sup headers.2)}}}: ' + Highcharts.numberFormat(this.points[0].point.q1, {{num decimalPlaces}}) + '<br/>' +
	    		'{{{sub (sup headers.3)}}}: ' + Highcharts.numberFormat(this.points[0].point.median, {{num decimalPlaces}}) + '<br/>' +
	    		'{{{sub (sup headers.4)}}}: ' + Highcharts.numberFormat(this.points[0].point.q3, {{num decimalPlaces}}) + '<br/>' +
	    		'{{{sub (sup headers.5)}}}: ' + Highcharts.numberFormat(this.points[0].point.high, {{num decimalPlaces}});
	    };
	    
options.legend.enabled=false;
options.plotOptions={
			boxplot: {
				animation:false,
                lineWidth: 2,
                medianWidth: 3,
                stemDashStyle: 'dash',
                stemWidth: 1,
                whiskerLength: '20%',
                whiskerWidth: 3
			}
		};
{{/partial}}
{{!-- This denotes the template inherits from base-chart-config--}}
{{> highcharts/config/base/base-chart-config}} 