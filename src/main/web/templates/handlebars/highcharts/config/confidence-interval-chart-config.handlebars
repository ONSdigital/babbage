{{#partial "chart-config"}}
options.labels={};
options.yAxis.title={
	align:"high",
	text:"{{unit}}"
};
options.chart.inverted=false;
options.series=[{
			name: "{{{headers.1}}}",
			type: "bar",
			data: [{{#each data}}{{num (get this headers.1)}},{{/each}}]
	    },{
	    	name: "{{headers.2}} - {{headers.3}}",
	    	type: "errorbar",
	    	data: [{{#each data}}[{{num (get this headers.2)}},{{num (get this headers.3)}}],{{/each}}]
	    }];
options.plotOptions.scatter={marker: {radius: 4}};
{{/partial}}

{{!-- This denotes the template inherits from base-chart-config--}}
{{> highcharts/config/base-chart-config}} 