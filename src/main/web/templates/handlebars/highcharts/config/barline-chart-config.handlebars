{{#partial "chart-config"}}
{
	chart: {
		height: {{width}} * {{aspectRatio}},
		width: {{width}}
	},
	colors: [
		"#274796",
		"#F5942F",
		"#E73F40",
		"#7BCAE2",
		"#979796",
		"#E9E117",
		"#74B630",
		"#674796",
		"#BD5B9E"
	],
	series: [{{#each series}}
	{
		name: "{{this}}",
		type: "{{#if_eq (get  chartTypes this) "bar"}}column{{else}}line{{/if_eq}}",
		zIndex: {{#if_eq (get  chartTypes this) "line"}}100{{else}}0{{/if_eq}},
		stack: "{{#contains groups.0 this}}group1{{else}}{{this}}{{/contains}}",
		data: [{{#each data}}{{nf (get this ..) alt="null"}}{{/each}}]
	},{{/each}} 
	],
	title: {
		text: ""
	},
	yAxis: {
		title: {
			text: ""
		},
		labels: {
			format: "{value:,f}"
		}
	},
	xAxis: {
		categories: [{{#each categories}}"{{this}}",{{/each}}],
		tickInterval: "{{labelInterval}}"
	},
	labels: {
		items: [{
			html: "{{unit}}",
			style: {
				left: "0px",
				top: "0px"
			}
		}]
	},
	legend: {
		verticalAlign: "top"
	},
	plotOptions: {
		series: {
			animation: false,
			pointPadding: 0,
			groupPadding: 0.1
		},
		line: {
			lineWidth: 1,
			marker: {
				radius: 2,
				symbol: "circle"
			}
		},
		column: {
			stacking: "normal"
		}
	},
	tooltip: {
		valueDecimals: "{{decimalPlaces}}",
		shared: true
	},
	credits: {
		enabled: false
	}
}
{{/partial}}

{{!-- This denotes the template inheritcs from base-chart-config--}}
{{> highcharts/config/base-chart-config}} 