{{#partial "chart-config"}}
options.series=[{{#each series}}{
		name: "{{{this}}}",
		type: "{{#if_eq (get  chartTypes this) "bar"}}{{#if rotated}}bar{{else}}column{{/if}}{{else}}line{{/if_eq}}",
		zIndex: {{#if_eq (get  chartTypes this) "line"}}100{{else}}0{{/if_eq}},
		stack: "{{#contains groups.0 this}}group1{{else}}{{this}}{{/contains}}",
		data: [{{#each data}}{{alt (get this ..) "null"}},{{/each}}]
	},{{/each}} ];

options.plotOptions.column = {stacking:"normal"};
{{/partial}}

{{!-- This denotes the template inheritcs from base-chart-config--}}
{{> highcharts/config/base-chart-config}} 