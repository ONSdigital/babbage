{{#partial "chart-config"}}
{
	chart: {
		height: {{width}} * {{aspectRatio}},
		width: {{width}}
	},
	colors: [
		"#274796",
		"#F5942F",
		"#E73F40",
		"#7BCAE2",
		"#979796",
		"#E9E117",
		"#74B630",
		"#674796",
		"#BD5B9E"
	],
	series: [
	{
		name: "{{headers.1}}",
		type: "{{#if_eq (get  chartTypes headers.1) "bar"}}column{{else}}line{{/if_eq}}",
		zIndex: {{#if_eq (get  chartTypes headers.1) "line"}}100{{else}}0{{/if_eq}},
		yAxis: 0,
		data: [{{#each data}}{{alt (get this headers.1) "null"}},{{/each}}]
	},
	{
		name: "{{headers.2}}",
		type: "{{#if_eq (get  chartTypes headers.2) "bar"}}column{{else}}line{{/if_eq}}",
		zIndex: {{#if_eq (get  chartTypes headers.2) "line"}}100{{else}}0{{/if_eq}},
		yAxis: 1,
		data: [{{#each data}}{{alt (get this headers.2) "null"}},{{/each}}]
	}
	],
	title: {
		text: ""
	},
	yAxis: [{
		title: {text: "{{headers.1}}"},
		labels: {format: "{value:,f}"}
		},
		{
		title: {text: "{{headers.2}}"},
		labels: {format: "{value:,f}"},
		opposite:true
	}],
	xAxis: {
		categories: [{{#each categories}}"{{this}}",{{/each}}],
		tickInterval: "{{labelInterval}}",
		crosshair: true
	},
	labels: {
		items: [{
			html: "{{unit}}",
			style: {
				left: "0px",
				top: "0px"
			}
		}]
	},
	legend: {
		verticalAlign: "top"
	},
	plotOptions: {
		series: {
			animation: false,
			pointPadding: 0,
			groupPadding: 0.1
		},
		line: {
			lineWidth: 1,
			marker: {
				radius: 2,
				symbol: "circle"
			}
		}
	},
	tooltip: {
		valueDecimals: "{{decimalPlaces}}",
		shared: true
	},
	credits: {
		enabled: false
	}
}
{{/partial}}

{{!-- This denotes the template inheritcs from base-chart-config--}}
{{> highcharts/config/base-chart-config}} 