{{#partial "chart-config"}}
delete options.chart.marginRight;

options.series=[{
		name: "{{{headers.1}}}",
		type: "{{#if_eq (get  chartTypes headers.1) "bar"}}column{{else}}line{{/if_eq}}",
		zIndex: {{#if_eq (get  chartTypes headers.1) "line"}}100{{else}}0{{/if_eq}},
		yAxis: 0,
		data: [{{#each data}}{{num (get this headers.1)}},{{/each}}]
	},{
		name: "{{{headers.2}}}",
		type: "{{#if_eq (get  chartTypes headers.2) "bar"}}column{{else}}line{{/if_eq}}",
		zIndex: {{#if_eq (get  chartTypes headers.2) "line"}}100{{else}}0{{/if_eq}},
		yAxis: 1,
		data: [{{#each data}}{{num (get this headers.2)}},{{/each}}]
	}];

options.yAxis=[
		{title: {text: "{{{headers.1}}}"},labels: {format: "{value:,f}"}},
		{title: {text: "{{{headers.2}}}"},labels: {format: "{value:,f}"},opposite:true}
		];

options.xAxis.crossHair=true;
{{/partial}}

{{!-- This denotes the template inherits from base-chart-config--}}
{{> highcharts/config/base-chart-config}} 