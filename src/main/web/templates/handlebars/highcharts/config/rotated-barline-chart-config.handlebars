{{#partial "chart-config"}}
options.labels={};
options.yAxis.title={
	align:"high",
	text:"{{unit}}"
};

options.series=[{{#each series}}{
		name: "{{{this}}}",
		type: "{{#if_eq (get  chartTypes this) "bar"}}bar{{else}}line{{/if_eq}}",
		zIndex: {{#if_eq (get  chartTypes this) "line"}}100{{else}}0{{/if_eq}},
		stack: "{{#contains groups.0 this}}group1{{else}}{{this}}{{/contains}}",
		data: [{{#each data}}{{num (get this ..)}},{{/each}}]
	},{{/each}} ];

options.plotOptions.bar = {stacking:"normal"};
{{/partial}}
{{!-- This denotes the template inherits from base-chart-config--}}
{{> highcharts/config/base-chart-config}} 