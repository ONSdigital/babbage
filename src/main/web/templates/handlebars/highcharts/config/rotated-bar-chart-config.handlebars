{{#partial "chart-config"}}
{  
    chart: {
        height: {{width}} * {{aspectRatio}},
        width: {{width}},
        marginRight: 35
    },
    colors:[  
        "#274796",
        "#F5942F",
        "#E73F40",
        "#7BCAE2",
        "#979796",
        "#E9E117",
        "#74B630",
        "#674796",
        "#BD5B9E"
    ],
    series: [{{#each series}}
    {
        name: "{{{this}}}",
        type: "bar",
        data: [{{#each data}}{{alt (get this ..) alt="null"}},{{/each}}]
    },{{/each}} 
    ],
    title:{  
        text:""
    },
    yAxis:{  
        title:{  
            text:"{{{unit}}}",
            align:"high"
        },
        labels:{  
            format:"{value:,f}"
        }
    },
    xAxis: {
        categories: [{{#each categories}}"{{{this}}}",{{/each}}],
        tickInterval: "{{labelInterval}}"
    },
    legend:{  
        verticalAlign:"top"
    },
    plotOptions:{  
        series:{  
            animation:false,
            pointPadding:0,
            groupPadding:0.1
        },
        line:{  
            lineWidth:1,
            marker:{  
                radius:2,
                symbol:"circle"
            }
        },
        column:{  

        }
    },
    tooltip: {
        valueDecimals: "{{decimalPlaces}}",
        shared: true
    },
    credits:{  
        enabled:false
    }
}
{{/partial}}

{{!-- This denotes the template inheritcs from base-chart-config--}}
{{> highcharts/config/base-chart-config}} 