{{#partial "additional-chart-config"}}
options.chart.type="{{#if area}}area{{else}}line{{/if}}";
			
options.xAxis.tickPositioner=function() {
          var positions = [];
          var tick = Math.floor(this.dataMax);
          for (tick; tick >= this.dataMin; tick -= this.tickInterval) {
            positions.push(tick);
          }
          return positions;
        }
{{/partial}}
