{{#partial "chart-config"}}
options.series = [{{#each series}}{
			name: "{{{this}}}",
			type: "{{#if area}}area{{else}}line{{/if}}",
			data: [{{#each data}}{{alt (get this ..) "null"}},{{/each}}]
		},{{/each}} ];
		
options.xAxis.tickPositioner=function() {
          var positions = [];
          var tick = Math.floor(this.dataMax);
          for (tick; tick >= this.dataMin; tick -= this.tickInterval) {
            positions.push(tick);
          }
          return positions;
        }
{{/partial}}
{{!-- This denotes the template inheritcs from base-chart-config--}}
{{> highcharts/config/base-chart-config}} 