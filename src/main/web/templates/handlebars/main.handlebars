    <script>
        $(function() {
            // on external links run the GA trackOutboundLink function
            $("a[target='_blank']").on( "click", function() {
                var href = $(this).attr("href");
                trackOutboundLink(href); //return false;
            });

            $('a[data-ga-event]').on("click", function() {
                var $this = $(this),
                    category = $this.attr('data-ga-event-category'),
                    label = $this.attr('data-ga-event-label');
                //console.log("You clicked " + $this.text() + " category: " + category + " label: " + label);
                trackEvent(category, label);
            });

        });
    </script>

