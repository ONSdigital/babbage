{{!-- Main handlebars is the main file rendered by handlebars.
performs type check on content type and decides which template to render  --}}
{{!-- Resolve parents of current uri for and assign to variable breadcrumb for building breadcrumb --}}
{{resolveParents uri assign="breadcrumb"}}
{{!-- Resolve navigation fetching children of root uri "/" with depth 2 --}}
{{resolveChildren "/" depth=2 assign="navigation"}}

{{> partials/header}}
{{#switch type}}
{{#case "home_page"}}{{> content/t1}}{{/case}}
{{#case "taxonomy_landing_page"}}{{> content/t2}}{{/case}}
{{#case "product_page"}}{{> content/t3}}{{/case}}
{{#case "bulletin"}}{{> content/t4-1}}{{/case}}
{{#case "article"}}{{> content/t4-2}}{{/case}}
{{#case "timeseries"}}{{> content/t5-1}}{{/case}}
{{#case "data_slice"}}{{> content/t5-2}}{{/case}}
{{#case "compendium_landing_page"}}{{> content/t6-1}}{{/case}}
{{#case "compendium_chapter"}}{{> content/t6-2}}{{/case}}
{{#case "compendium_data"}}{{> content/t6-3}}{{/case}}
{{#case "reference_tables"}}{{> content/t8-1}}{{/case}}
{{#case "dataset"}}{{> content/t8-3}}{{/case}}
{{#case "timeseries_dataset"}}{{> content/t8-3}}{{/case}}
{{#case "static_landing_page"}}{{> content/t7-4-1}}{{/case}}
{{#case "static_page"}}{{> content/t7-5}}{{/case}}
{{#case "static_article"}}{{> content/t7-6}}{{/case}}
{{#case "static_methodology"}}{{> content/t7-6}}{{/case}}
{{#case "static_qmi"}}{{> content/t7-1}}{{/case}}
{{#case "static_adhoc"}}{{> content/t7-2}}{{/case}}
{{#case "static_foi"}}{{> content/t7-3}}{{/case}}
{{#case "release"}}{{> content/t16}}{{/case}}
{{/switch}}
{{> partials/footer}}