{{#if_eq type "product_page"}}
    {{> partials/breadcrumb-neutral}}
{{else if_eq type "bulletin"}}
    {{> partials/breadcrumb-bulletin}}
{{else if isPrototypeArticle}}
    {{> partials/breadcrumb-article}}
{{else if_eq type "timeseries"}}
    {{#if previous}}
        {{> list/partials/list-page-breadcrumb label="Previous versions" galleryBackground=true}} 
    {{else}}		
        {{> partials/breadcrumb}}
    {{/if}}
{{else if_eq type "dataset"}} 	
    {{> list/partials/list-page-breadcrumb label=description.edition galleryBackground=true}}					
{{else if_eq type "timeseries_dataset"}} 			
    {{> list/partials/list-page-breadcrumb label=description.edition galleryBackground=true}}
{{else}}
    {{#if_ne type "error"}}
        {{> partials/breadcrumb}}
    {{/if_ne }}
{{/if_eq}}
