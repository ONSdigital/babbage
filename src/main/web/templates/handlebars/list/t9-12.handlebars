{{#partial "block-main"}}
	<div class="page-intro">
		<div class="wrapper">
			<div class="col-wrap">
				<div class="col one">
					{{> list/partials/list-page-breadcrumb label="Commissioned data"}}
					<h1 class="page-intro__title margin-bottom--double">Commissioned data</h1>
				</div>
			</div>
		</div>
	</div>

	<div class="wrapper print-links">
    	<form id="filters">
			<div class="col-wrap print-hidden">
				<div class="col">
					<div class="float-right">
						{{> list/filters/sort}}
					</div>
				</div>
			</div>
			<div class="col col--md-12 col--lg-18 margin-bottom">
				<div class="tiles__item tiles__item--nav-type flush-col print--hide">
					<div class="tiles__title-h3 tiles__title-h3--nav">
						<h3 class="inline">Refine results</h3>
							<a href="{{location.path}}" value="Reset" class="btn btn--primary btn--thin btn--narrow btn--small float-right">{{labels.search-refine-clear}}</a>
					</div>
					<div class="tiles__content tiles__content--nav">
						<div class="filters">
							{{> list/filters/keywords}}
							{{> list/filters/publish-date}}
							{{> list/filters/topics}}
						</div>
						<button type="submit" class="btn btn--primary btn--thick margin-top--double">Filter</button>
					</div>
				</div>
				{{> partials/archives-box}}
			</div>
			<div class="results col col--md-34 col--lg-40 margin-left-md--1">
				{{> list/partials/results-text}}
				{{> list/partials/results}}
				{{> list/partials/paginator}}
			</div>
		</form>
	</div>
{{/partial}}
	{{!-- Inheriting from base list template --}}
	{{> list/base/list}}