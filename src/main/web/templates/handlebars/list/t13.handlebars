<div id="timeseries-list-container"  class="width--md-35 width--lg-41 background--iron-light padding-top-md--3 padding-right-md--1 padding-bottom-md--3 padding-left-md--1 flush" style="display:none" >
        <ul id="timeseries-list" class="list--neutral margin-top-md--0 margin-left-md--0 border-bottom--silver-md border-bottom--silver-lg">
        </ul>

        <p class="flush inline margin-right-md--1">Download selected time series</p>

        {{!-- Excel download --}}
        <form id="xls-form" method="post" action="/export" class="inline print--hide">
            <button type="submit" title="Download XLS" class="btn btn--primary download-analytics">
                Download .xls
            </button>
            <input type="hidden" name="format" value="xls"/>
        </form>

        {{!-- CSV download --}}
        <form id="csv-form" method="post" action="/export" class="inline print--hide">
            <button type="submit" title="Download CSV" class="btn btn--primary download-analytics">
                Download .csv
            </button>
            <input type="hidden" name="format" value="csv"/>
        </form>
</div>

{{#partial "block-title"}}
Time series data explorer
{{/partial}}

{{#partial "block-results-text"}}
	<div class="col col--md-28 col--lg-38">
		{{> list/partials/list-results-text-number result resultsLabel="time series data" showTopic=true}}
	</div>
{{/partial}}

{{#partial "block-filters"}}
	{{> list/filters/keywords}}
    {{> list/filters/topics}}
	{{> list/filters/update-date}}
{{/partial}}

{{#partial "block-results"}}
        
        
        <div id="select-all-container" class="js-enhance--show">
        {{#if result.results}}
            <input id="select-all"  type="checkbox" name="" data-title="{{description.title}}" data-uri="{{uri}}"  value="" class="timeseriestool-select js-timeseriestool-select-all" aria-label="select all">
            <label for="select-all">Select all</label>
        {{/if}}
        </div>
        
        {{#each result.results}}
            <div class="col-wrap {{#unless @first}}border-top--iron-md{{/unless}}">

                {{!-- Checkbox --}}
                <div class="col col--md-3 col--lg-3 margin-left-md--1 padding-left-md--1">
                    <input  type="checkbox" name="" data-title="{{description.title}}" data-cdid="{{description.cdid}}" data-uri="{{uri}}"  value="" class="timeseriestool-select js-timeseriestool-select" aria-label="{{description.title}}">
                </div>

                {{!-- Title --}}
                <div class="col col--md-21 col--lg-25 padding-right-md--1">
                    <p class="margin-bottom-md--0"><a href="{{uri}}">{{{description.title}}}</a></p>
                </div>

                {{!-- Is new? --}}
                <div class="col col--md-2 col--lg-2">
                    {{#if (last24hours description.releaseDate)}}
                    <p class="margin-bottom-md--0">NEW</p>
                    {{/if}}
                </div>

                {{!-- Release date --}}
                <div class="col col--md-8 col--lg-10 padding-right-md--1">
                    <p class="margin-bottom-md--0 text-right">{{df description.releaseDate}}</p>
                </div>

                <div class="col-wrap">

                    {{!-- CDID --}}
                    <div class="col col--md-7 col--lg-7 margin-left-md--5 margin-bottom-md--2">
                        <p class="search-results__meta">Series ID: {{description.cdid}}</p>
                    </div>

                    {{!-- Unit --}}
                    <div class="col col--md-4 col--lg-4">
                        <p class="search-results__meta">{{#if description.unit}}
                           | Units: {{description.unit}}
                        {{else}}
                            &nbsp;
                        {{/if}}</p>
                    </div>

                </div>

            </div>
        {{/each}}


{{/partial}}

{{!-- Inheriting from base list template --}}
{{> list/base/list }}


