{{#partial "block-results-text"}}
	<div class="col col--md-28 col--lg-38">
		<p>You can find all of the statistical releases we produce in this comprehensive list.</p>
	</div>
{{/partial}}

{{#partial "block-sort"}}
	
{{/partial}}

{{#partial "block-filters"}}
	<div class="tiles__content tiles__content--nav">
		<div class="filters">
			{{> list/filters/keywords}}
			<h4 class="filters__title">Choose a letter to browse</h4>
			<div class="filters__a-z">
				{{> partials/az class="filters"}}
			</div>
		</div>
		<input type="hidden" name="q" value="{{parameters.q.0}}">
		<button type="submit" class="btn btn--primary btn--thick margin-top-sm--2 margin-top-md--2 js-enhance--hide">{{labels.search-filter}}</button>
	</div>
{{/partial}}

{{!-- Inheriting from base list template --}}
{{> list/base/list listLabel="Browse our statistics by A to Z"}}