<main id="main" role="main" tabindex="-1">
<div class="page-header slate slate--light-green">
    <div class="wrapper">
        {{> partials/breadcrumb}}
        <p class="page-header__title">
            {{labels.dataset}}
        </p>
        <h1 class="page-header__title flush--half--bottom" id="title">{{description.title}}</h1>
        <div class="grid-wrap mini">
            <p class="grid-col tablet-grid-one-quarter {{#if description.nationalStatistic}}page-header__meta-block-with-img{{/if}}">
                {{#if description.nationalStatistic}}
                <img src="/node_modules/ONS-Pattern-Library/dist/img/national-statistics.png" alt="National Statistics logo">
                {{/if}}
                <span>{{labels.dataset-contact}}: </span><br/><a href="mailto:{{description.contact.email}}">{{#if
            description.contact.name}}{{description.contact.name}}{{else}}{{description.contact.email}}{{/if}}</a>
            <p class="grid-col tablet-grid-one-quarter divider--border-left">
                <span>{{labels.release-date}}: </span><br/>{{df description.releaseDate}}
                </br>
                {{!--<a class="print-hidden" href="#">previous versions</a>--}}
            </p>
            <p class="grid-col tablet-grid-one-quarter divider--border-left">
                <span>{{labels.release-next}}: </span><br/>{{#if description.nextRelease}}{{description.nextRelease}}{{else}}To be announced{{/if}}
            </p>
            {{#if description.datasetId}}
            <p class="grid-col tablet-grid-one-quarter divider--border-left">
                <span>{{labels.dataset-id}}: </span>{{description.datasetId}}
                <a href="#" class="tooltip nojs-hidden" title="This series ID is a unique random identifier for this individual time series. The digits themselves do not have any specific meaning" aria-label="This series ID is a unique random identifier for this individual time series. The digits themselves do not have any specific meaning">{{labels.dataset-what-is}}</a>
            </p>
            {{/if}}
        </div>
        <!-- /grip-wrap -->
    </div>
    <!-- / wrapper -->
</div>
<!-- / page-header -->
<div class="wrapper panel--mar print-links">
    <div class="grid-wrap">
        <div class="grid-col desktop-grid-full-width">
            <!-- /box -->
            <div class="lede">
                {{md description.summary}}
            </div>
        </div>
        <!-- /grid-col -->
        <div class="grid-col desktop-grid-two-thirds">
            <div class="wrapper panel--pad-small panel--bottom-mar muted--dark">
                <h3>{{labels.dataset-downloads}}</h3>
                <ul class="list--neutral">
                    {{#each downloads}}
                    {{#if file}}
                    <li>
                        <article class="box box--padded--med solid">
                            <div class="box--content">
                                <div class="grid-wrap">
                                    <div class="grid-col tablet-grid-one-quarter">
                                        {{#if_eq type 'timeseries_dataset'}}{{labels.dataset-latest}}{{else}}{{title}}{{/if_eq}}
                                    </div>
                                    <div class="grid-col tablet-grid-three-quarters">
                                        <form method="get" action="/file">
                                            <!-- link to given uris of given files-->
                                            <button type="submit" title="Latest version" class="btn btn--primary btn--block btn--thin download-analytics">
                                            {{fe file}} ({{fs file}})
                                            <span class="icon icon-download" role="presentation"></span>
                                            </button>
                                            <input  type="hidden" name="uri" value="{{file}}"/>
                                        </form>

                                    </div>
                                </div>
                                <!-- /grid-wrap -->
                            </div>
                            <!-- /box--content -->
                        </article>
                        <!-- /box -->
                    </li>
                    {{/if}}
                    {{/each}}
                </ul>
            </div>
            {{#if section.markdown}}
            <div class="panel--bottom-mar">
                <h3>{{labels.dataset-notes}}</h3>
                {{md section.markdown}}
            </div>
            {{/if}}
        </div>
        <!-- /grid-col desktop-grid-two-thirds -->
        <div class="grid-col desktop-grid-one-third">
            <!-- RELATED DATASETS -->
            {{#if relatedDatasets}}
            <article class="box solid box--green--separated--left">
                <div class="box__inner border box--padded">
                    <header class="box__header">
                        <h3>{{labels.dataset-related-data}}</h3>
                    </header>
                    <div class="box__content">
                        <ul class="list--neutral">
                            {{#each relatedDatasets}}
                            {{#resolve this.uri filter="title"}}
                            <li class="flush--third--bottom"><a href="{{absolute uri}}" title="{{title}}">{{title}}</a></li>
                            {{/resolve}}
                            {{/each}}
                        </ul>
                    </div>
                </div>
                <!-- /box__inner -->
            </article>
            {{/if}}
            <!-- RELATED BULLETINS & ARTICLES -->
            {{#if relatedDocuments}}
            <article class="box solid box--teal--separated--left">
                <div class="box__inner border box--padded">
                    <header class="box__header">
                        <h3>{{labels.dataset-related-a-b}}</h3>
                    </header>
                    <div class="box__content">
                        <ul class="list--neutral">
                            {{#each relatedDocuments}}
                            {{#resolve this.uri filter="title"}}
                            <li class="flush--third--bottom">
                                <a href="{{uri}}" title="{{title}}">{{title}}
                                {{#if edition}}
                                    {{#if_ne edition 'yes'}}: {{edition}}{{/if_ne}}
                                {{/if}}
                                </a>
                            </li>
                            {{/resolve}}
                            {{/each}}
                        </ul>
                    </div>
                </div>
                <!-- /box__inner -->
            </article>
            {{/if}}
            <!-- /box -->
            {{> partials/local-box}}
        </div>
        <!-- /grid-col -->
        <div class="grid-col desktop-grid-full-width">
            <div class="grid-wrap">
                <div class="grid-col desktop-grid-one-half">
                    <!--{{#if relatedMethodology}}-->
                    <article class="box box--padded sectioned">
                        <header class="box__header">
                            <h3>{{labels.methodology}}</h3>
                        </header>
                        <div class="box__content">
                            <ul class="list--neutral">
                                {{#each relatedMethodology}}
                                {{#resolve this.uri filter="title"}}
                                <li class="flush--third--bottom">
                                    <a href="{{uri}}" title="{{title}}">{{title}}</a>
                                </li>
                                {{/resolve}}
                                {{/each}}
                            </ul>
                        </div>
                    </article>
                    <!--{{/if}}-->
                </div>
                <!-- /grid-desktop-one-half -->
                <div class="grid-col desktop-grid-one-half">
                    <article class="box box--padded sectioned">
                        <header class="box__header">
                            <h3>{{labels.dataset-contact}}</h3>
                        </header>
                        <div class="box__content">
                            <address>
                                {{#if description.contact.name}}{{description.contact.name}}</br>{{/if}}
                                <a href="mailto:{{description.contact.email}}">{{description.contact.email}}</a></br>
                                {{#if description.contact.telephone}}{{labels.phone}}: {{description.contact.telephone}}{{/if}}
                            </address>
                        </div>
                    </article>
                </div>
                <!-- /grid-desktop-one-half -->
            </div>
            <!-- /grid-wrap -->
        </div>
        <!--/grid-col desktop-grid-full-width -->
    </div>
    <!-- /grid-wrap -->
</div>
<!-- /wrapper -->
</main>