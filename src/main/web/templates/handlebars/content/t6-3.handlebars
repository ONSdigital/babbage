<main id="main" role="main" tabindex="-1">
<div class="page-header slate slate--light-green">
	<div class="wrapper">
		{{> partials/breadcrumb}}
		<p class="page-header__title">
			{{labels.dataset}}
		</p>
		<h1 class="page-header__title flush--half--bottom">{{description.title}}</h1>
		<div class="grid-wrap mini">
			<p class="grid-col tablet-grid-one-quarter {{#if description.nationalStatistic}}page-header__meta-block-with-img{{/if}}">
				{{#if description.nationalStatistic}}
				<img src="/node_modules/ONS-Pattern-Library/ui/img/national-statistics.png" alt="National Statistics logo">
				{{/if}}
				<span>{{labels.release-contact}} </span><br/><a href="mailto:{{description.contact.email}}">{{#if description.contact.name}}{{description.contact.name}}{{else}}{{description.contact.email}}{{/if}}</a>
			</p>
			<p class="grid-col tablet-grid-one-quarter divider--border-left">
				<span>{{labels.release-date}} </span><br/>{{df description.releaseDate}}
				</br>
				<a class="print-hidden" href="#">{{labels.release-prev}}</a>
			</p>
			{{#if description.nextRelease}}
			<p class="grid-col tablet-grid-one-quarter divider--border-left">
				<span>{{labels.release-next}} </span><br/>{{description.nextRelease}}
			</p>
			{{/if}}
			<p class="grid-col tablet-grid-one-quarter divider--border-left">
				<span>{{labels.dataset-id}}: </span>{{description.datasetId}}
        <a href="#" class="tooltip" title="This series ID is a unique random identifier for this individual time series. The digits themselves do not have any specific meaning" aria-label="This series ID is a unique random identifier for this individual time series. The digits themselves do not have any specific meaning">{{labels.dataset-what-is}}â€¨</a>
			</p>
		</div>
		<!-- /grip-wrap -->
	</div>
	<!-- / wrapper -->
</div>
<!-- / page-header -->
<div class="wrapper panel--mar print-links">
	<div class="grid-wrap">
		<div class="grid-col desktop-grid-full-width">
      <h2 class="flush">
				{{#each breadcrumb}}
					{{#if @last}}
            &lt; {{labels.compendium-return}} <a href="{{uri}}">{{description.title}}</a>
					{{/if}}
				{{/each}}
      </h2>
			<!-- /box -->
			<div class="lede">
				{{md description.summary}}
			</div>
			<p>
				<span class="stand-out-text">
					{{#each downloads}}
						{{#if @last}}
							{{#if_eq @index_1 '1'}}
								{{@index_1}}</span> result
							{{/if_eq}}
							{{#if_ne @index_1 '1'}}
								{{@index_1}}</span> results
							{{/if_ne}}
						{{/if}}
					{{/each}}
			</p>
		</div>
		<!-- /grid-col -->
		<div class="grid-col desktop-grid-two-thirds">
			<ul class="list--neutral">
				{{#each downloads}}
				<li>
					<h3 class="flush"><a href="/file?uri={{file}}" title="{{title}}">{{title}}</a></h3>
					{{md fileDescription}}
				</li>
				{{/each}}
			</ul>
		</div>
		<!-- /grid-col desktop-grid-two-thirds -->
		<div class="grid-col desktop-grid-one-third">
      <!-- RELATED DATASETS -->
			{{#if relatedDatasets}}
        <article class="box solid box--green--separated--left">
          <div class="box__inner border box--padded">
            <header class="box__header">
              <h3>Related datasets</h3>
            </header>
            <div class="box__content">
              <ul class="list--neutral">
								{{#each relatedDatasets}}
                  <li class="flush--third--bottom"><a href="{{absolute uri}}" title="{{description.title}}">{{description.title}}</a></li>
								{{/each}}
              </ul>
            </div>
          </div>
          <!-- /box__inner -->
        </article>
			{{/if}}
      <!-- RELATED BULLETINS & ARTICLES -->
			{{#if relatedDocuments}}
        <article class="box solid box--teal--separated--left">
          <div class="box__inner border box--padded">
            <header class="box__header">
              <h3>Statistical bulletins and articles that use this time series</h3>
            </header>
            <div class="box__content">
              <ul class="list--neutral">
								{{#each relatedDocuments}}
                  <li class="flush--third--bottom">
                    <a href="{{uri}}" title="{{description.title}}">{{description.title}}
											{{#if description.edition}}
												{{#if_ne description.edition 'yes'}}: {{description.edition}}{{/if_ne}}
											{{/if}}
                    </a>
                  </li>
								{{/each}}
              </ul>
            </div>
          </div>
				<!-- /box__inner -->
			</article>
			{{/if}}
			<!-- /box -->
		</div>
		<!-- /grid-col -->
		<div class="grid-col desktop-grid-full-width">
			<div class="grid-wrap">
				<div class="grid-col desktop-grid-one-half">
					<!--{{#if relatedMethodology}}-->
					<article class="box box--padded sectioned">
						<header class="box__header">
							<h3>Methodology</h3>
						</header>
						<div class="box__content">
							<ul class="list--neutral">
								{{#each relatedMethodology}}
								<li class="flush--third--bottom">
									<a href="{{uri}}" title="{{description.title}}">{{description.title}}</a>
								</li>
								{{/each}}
							</ul>
						</div>
					</article>
					<!--{{/if}}-->
				</div>
				<!-- /grid-desktop-one-half -->
				<div class="grid-col desktop-grid-one-half">
					<article class="box box--padded sectioned">
						<header class="box__header">
							<h3>Contact details</h3>
						</header>
						<div class="box__content">
							<address>
								{{#if description.contact.name}}{{description.contact.name}}</br>{{/if}}
								<a href="mailto:{{description.contact.email}}">{{description.contact.email}}</a></br>
								{{#if description.contact.telephone}}Telephone: {{description.contact.telephone}}{{/if}}
							</address>
						</div>
					</article>
				</div>
				<!-- /grid-desktop-one-half -->
			</div>
			<!-- /grid-wrap -->
		</div>
		<!--/grid-col desktop-grid-full-width -->
	</div>
	<!-- /grid-wrap -->
</div>
<!-- /wrapper -->
</main>