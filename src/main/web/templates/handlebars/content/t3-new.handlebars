{{#partial "block-content"}}
	{{!-- Page header
	<div class="page-intro">
		<div class="wrapper">
			<div class="col-wrap">
				<div class="col">

					{{> partials/breadcrumb}}


					<h1 class="page-intro__title">{{description.title}}</h1>
					<p class="page-intro__content col col--md-47 col--lg-48">{{description.summary}}</p>

				</div>
			</div>
		</div>

		{{!-- On this page and quick links
		<div class="nav-secondary-wrap">
			<div class="wrapper">
				<div class="col-wrap">
					<nav class="nav-secondary nav-secondary--border-right-lg col col--lg-half">
						<h2 class="nav-secondary__title">On this page:</h2>
						<ul class="nav-secondary__list">
							{{resolve items.0.uri assign="data"}}
							{{!-- <li class="nav-secondary__item"><a href="#latest-publication" class="jsEnhanceAnimateScroll">Highlights</a></li>
							{{#if items}}<li class="nav-secondary__item"><a href="#timeseries" class="jsEnhanceAnimateScroll">Time series data</a></li>{{/if}}
                            {{#if datasets}}<li class="nav-secondary__item"><a href="#datasets" class="jsEnhanceAnimateScroll">Spreadsheets</a></li>{{/if}}
							<li class="nav-secondary__item"><a href="#publications" class="jsEnhanceAnimateScroll">Publications</a></li>
						</ul>
					</nav>
					<nav class="nav-secondary col col--lg-half">
						<h2 class="nav-secondary__title">Quick links:</h2>
						<ul class="nav-secondary__list">
							{{#if_any items datasets}}<li class="nav-secondary__item"><a href="{{uri}}/datalist">All data in this topic</a></li>{{/if_any}}
							<li class="nav-secondary__item"><a href="{{uri}}/publications">All publications in this topic</a></li>
						</ul>
					</nav>
				</div> {{!-- /col-wrap --}}
			</div> {{!-- /wrapper --}}
		</div> {{!-- /nav-secondary --}}
	</div> {{!-- /page-intro --}}


<div class="border-top--iron-sm border-top--iron-md border-top--iron-lg overflow--hidden">
    <div class="wrapper">
        <div class="col-wrap">

            {{!-- On this page side bar--}}

            <div class="col col--md-7 col--lg-14 margin-right-md--2 margin-left-md--1 col margin-right-lg--2 margin-right-md--1">
                <h2 class="">On this page:</h2>
                <ul class="list--neutral">
                    {{resolve items.0.uri assign="data"}}
                    {{!-- <li class="nav-secondary__item"><a href="#latest-publication" class="jsEnhanceAnimateScroll">Highlights</a></li> --}}
                    {{#if items}}<li class=""><a href="#timeseries" class="jsEnhanceAnimateScroll">Time series data</a></li>{{/if}}
                    {{#if datasets}}<li class=""><a href="#datasets" class="jsEnhanceAnimateScroll">Spreadsheets</a></li>{{/if}}
                    <li class=""><a href="#publications" class="jsEnhanceAnimateScroll">Publications</a></li>
                </ul>
            </div>

            {{!-- Page content --}}

            <div class="col col--md-37 col--lg-42 border-left--iron-sm border-left--iron-md border-left--iron-lg">

                {{!-- Latest data/publication block --}}
                <div class="page-content padding-left-md--1 padding-bottom-md--2 overflow--hidden">
                    <h2>Highlights</h2>
                    <div class="col-wrap margin-left-lg--0">
                        {{resolve statsBulletins.0.uri filter="description" assign="bulletin"}}
                        {{#if bulletin}}
                            {{resolve items.0.uri assign="data"}}
                            <!-- <div id="latest" class="col col--md-half col--lg-half tiles__item tiles__item--image-type-headline"> -->

                            {{!-- Check for data and print partial --}}
                            {{#if_any data.years data.quarters data.months}}
                                <div class="col col--md-36 col--lg-20">
                                    {{> partials/t3/new-headline-data}}
                                </div>
                            {{/if_any}}
                            <!-- </div> -->

                            {{!-- Latest publication block --}}
                            <div class="col {{#if_any data.years data.quarters data.months}} col--lg-20 margin-left-md--1{{else}}col--lg-41{{/if_any}}">
                                <!--<div class="clickable-wrap">-->
                                    <div class="slate--iron-light margin-bottom-md--2">
                                        <div class="tiles__image height--24 overflow--hidden">

                                            <img src="{{#if_any (eq 'localhost' location.hostname) (eq '127.0.0.1' location.hostname)}}http://localhost:9000/dist/img/latest-publications.png{{else}}/node_modules/sixteens/dist/img/latest-publications.png{{/if_any}}" class="height--24" alt="" />
                                        </div>
                                        <div class="equal-height box__content padding-top-md--2 padding-right-md--1 padding-bottom-md--2 padding-left-md--1">
                                            <h3>
                                                <a href="{{bulletin.uri}}">{{bulletin.description.title}}{{#if bulletin.description.edition}}: {{bulletin.description.edition}}{{/if}}</a>
                                            </h3>
                                            <h4>{{labels.product-whats-in-bulletin}}</h4>
                                            <ul>
                                                {{#if bulletin.description.headline1}}
                                                    <li>{{bulletin.description.headline1}}</li>
                                                {{/if}}
                                                {{#if bulletin.description.headline2}}
                                                    <li>{{bulletin.description.headline2}}</li>
                                                {{/if}}
                                                {{#if bulletin.description.headline3}}
                                                    <li>{{bulletin.description.headline3}}</li>
                                                {{/if}}
                                            </ul>
                                            </div>
                                        <a href="{{bulletin.uri}}" class="slate--iron-light border-top--iron-md border-top--iron-sm">Read this statistical bulletin</a>
                                    </div>
                                <!--</div>-->
                                <a href="#publications" class="btn btn--primary btn--full-width jsEnhanceAnimateScroll">More publications</a>
                            </div>
                        {{/if}}
                    </div>

                {{!-- Timeseries list --}}
                {{#if items}}
                    {{> partials/t3/new-timeseries-list}}
                {{/if}}

                {{!-- Datasets list --}}
                {{#if datasets}}
                    {{> partials/t3/new-dataset-list}}
                {{/if}}

                {{!-- Publications list --}}
                {{#if_any statsBulletins relatedArticles}}
                    {{> partials/t3/new-publications-list}}
                {{/if_any}}

        {{!-- Related methodology --}}
		{{#if relatedMethodology}}
        <section class="margin-top">
            <div class="wrapper">
                <div class="col-wrap">
                    <div class="col col--lg-two-thirds">
                        <div class="tiles__item tiles__item--nav-type flush-col">
                            <h3 class="tiles__title-h3 tiles__title-h3--nav">Methodology related to {{description.title}}</h3>
                                <ul class="tiles__content tiles__content--nav list--neutral">
                                    {{#each relatedMethodology}}
                                    {{#resolve this.uri filter="description"}}
                                    <li class="flush-top"><a href="{{absolute uri}}">{{description.title}}</a></li>
                                    {{/resolve}}
                                    {{/each}}
                                </ul>
                            </div>
                        </div>
                    <div class="col col--lg-one-third">
                        {{> partials/local-box}}
                    </div>
                </div> {{!-- /col-wrap --}}
            </div>
        </section> {{!-- /section --}}
		{{/if}}
    </div>
    </div>
</div>
</div> {{!-- /col-wrap --}}
</div>{{!-- /wrapper --}}
</div>
{{/partial}}
{{> content/base/content}}
