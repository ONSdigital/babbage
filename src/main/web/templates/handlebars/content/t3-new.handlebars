{{#partial "block-content"}}

<div class="border-top--iron-sm border-top--iron-md border-top--iron-lg overflow--hidden">
    <div class="wrapper">
        <div class="col-wrap">

            {{!-- On this page side bar--}}

            <div class="col col--md-7 col--lg-17 margin-left-md--1">

                <div class="side-bar js-sticky-sidebar">
                    <h2 class="">On this page:</h2>
                    <ul class="list--neutral">
                        {{resolve items.0.uri assign="data"}}
                        <a href="#highlights" class="jsEnhanceAnimateScroll"><li class="side-bar__item" id="highlights-menu-item" >Highlights</li></a>
                        {{#if items}}<a href="#timeseries" class="jsEnhanceAnimateScroll"><li id="timeseries-menu-item" class="side-bar__item">Time series data</li></a>{{/if}}
                        {{#if datasets}}<a href="#datasets" class="jsEnhanceAnimateScroll"><li id="datasets-menu-item" class="side-bar__item">Spreadsheets</li></a>{{/if}}
                        <a href="#commissioned" class="jsEnhanceAnimateScroll"><li id="commissioned-menu-item" class="side-bar__item">Commissioned Statistics</li></a>
                        <a href="#publications" class="jsEnhanceAnimateScroll"><li id="publications-menu-item" class="side-bar__item">Publications</li></a>
                    </ul>
                </div>
            </div>

            {{!-- Page content --}}

            <div class="col col--md-37 col--lg-42 border-left--iron-sm border-left--iron-md border-left--iron-lg">

                {{!-- Latest data/publication block --}}
                <div class="page-content padding-left-md--1 padding-bottom-md--2 overflow--hidden js-sticky-scroll">
                    <div id="highlights-container" class="section-container">
                        <h2 id="highlights" class="section-title">Highlights</h2>
                        <div class="col-wrap margin-left-lg--0">
                            {{resolve statsBulletins.0.uri filter="description" assign="bulletin"}}
                            {{#if bulletin}}
                                {{resolve items.0.uri assign="data"}}
                                <!-- <div id="latest" class="col col--md-half col--lg-half tiles__item tiles__item--image-type-headline"> -->

                                {{!-- Check for data and print partial --}}
                                {{#if_any data.years data.quarters data.months}}
                                    <div class="col col--md-36 col--lg-20">
                                        {{> partials/t3/new-headline-data}}
                                    </div>
                                {{/if_any}}
                                <!-- </div> -->

                                {{!-- Latest publication block --}}
                                <div class="col {{#if_any data.years data.quarters data.months}}col--lg-20 margin-left-md--1{{else}}col--lg-41{{/if_any}}">
                                    <!--<div class="clickable-wrap">-->
                                        <div class="background--iron-light margin-bottom-md--2">
                                            <div class="background--abbey height--24 overflow--hidden">

                                                <img src="{{#if_any (eq 'localhost' location.hostname) (eq '127.0.0.1' location.hostname)}}http://localhost:9000/dist/img/latest-publications.png{{else}}/node_modules/sixteens/dist/img/latest-publications.png{{/if_any}}" class="height--24 width--20" alt="" />
                                            </div>
                                            <div class="equal-height box__content padding-top-md--2 padding-right-md--1 padding-bottom-md--2 padding-left-md--1">
                                                <h3>
                                                    <a href="{{bulletin.uri}}">{{bulletin.description.title}}{{#if bulletin.description.edition}}: {{bulletin.description.edition}}{{/if}}</a>
                                                </h3>
                                                <h4>{{labels.product-whats-in-bulletin}}</h4>
                                                <ul>
                                                    {{#if bulletin.description.headline1}}
                                                        <li>{{bulletin.description.headline1}}</li>
                                                    {{/if}}
                                                    {{#if bulletin.description.headline2}}
                                                        <li>{{bulletin.description.headline2}}</li>
                                                    {{/if}}
                                                    {{#if bulletin.description.headline3}}
                                                        <li>{{bulletin.description.headline3}}</li>
                                                    {{/if}}
                                                </ul>
                                                </div>
                                            <div class="background--iron-light box__content height--9 width--50 border-top--iron-sm border-top--iron-md padding-top-md--2 padding-left-md--1">
                                                <a href="{{bulletin.uri}}" class="block">Read this statistical bulletin</a>
                                            </div>
                                        </div>
                                    <!--</div>-->
                                    <a href="#publications" class="btn btn--primary btn--full-width jsEnhanceAnimateScroll">More publications</a>
                                </div>
                            {{/if}}
                        </div>
                    </div>

                {{!-- Timeseries list --}}
                {{#if items}}
                    <div id="timeseries-container" class="section-container">
                    {{> partials/t3/new-timeseries-list}}
                    </div>
                {{/if}}

                {{!-- Datasets list --}}
                {{#if datasets}}
                    <div id="datasets-container" class="section-container">
                    {{> partials/t3/new-dataset-list}}
                    </div>
                {{/if}}


                    <div id="commissioned-container" class="section-container">
                        {{!-- Commissioned Statistics --}}
                        <h2 id="commissioned" class="section-title">Commissioned Statistics</h2>
                        <div class="background--iron-light margin-bottom-md--2">

                            <div class="box__content padding-top-md--3 padding-right-md--1 padding-bottom-md--2 padding-left-md--1">
                                <h3>Looking for our user requested data?</h3>
                                <p><a href="aboutus/whatwedo/statistics/requestingstatistics">We also product statistics not included in our standard publications</a></p>
                            </div>

                        </div>
                    </div>


                {{!-- Publications list --}}
                {{#if_any statsBulletins relatedArticles}}
                    <div id="publications-container" class="section-container">
                        {{> partials/t3/new-publications-list}}
                    </div>
                {{/if_any}}

        {{!-- Related methodology --}}
		{{#if relatedMethodology}}
        <section class="margin-top">
            <div class="wrapper">
                <div class="col-wrap">
                    <div class="col col--lg-two-thirds">
                        <div class="tiles__item tiles__item--nav-type flush-col">
                            <h3 class="tiles__title-h3 tiles__title-h3--nav">Methodology related to {{description.title}}</h3>
                                <ul class="tiles__content tiles__content--nav list--neutral">
                                    {{#each relatedMethodology}}
                                    {{#resolve this.uri filter="description"}}
                                    <li class="flush-top"><a href="{{absolute uri}}">{{description.title}}</a></li>
                                    {{/resolve}}
                                    {{/each}}
                                </ul>
                            </div>
                        </div>
                    <div class="col col--lg-one-third">
                        {{> partials/local-box}}
                    </div>
                </div> {{!-- /col-wrap --}}
            </div>
        </section> {{!-- /section --}}
		{{/if}}
    </div>
    </div>
</div>
</div> {{!-- /col-wrap --}}
</div>{{!-- /wrapper --}}
</div>
{{/partial}}
{{> content/base/content}}
