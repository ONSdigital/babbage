{{!-- Page header
<div class="page-intro">
    <div class="wrapper">
        <div class="col-wrap">
            <div class="col">
                {{!-- Breadcrumb
                {{> list/partials/list-page-breadcrumb label=description.edition}}

                {{!-- Title & summary
                <h1 class="page-intro__title {{#unless description.summary}}margin-bottom{{/unless}}">
                    <span class="page-intro__type">Spreadsheet version:</span>
                    {{#each breadcrumb}}
                        {{#if @last}}
                            {{description.title}}
                        {{/if}}
                    {{/each}}
                    {{#if description.edition}}: {{description.edition}}{{/if}}
                </h1>

                {{#if description.summary}}
                    <p class="page-intro__content col col--md-47 col--lg-48">{{description.summary}}</p>
                {{/if}}
				{{meta}}
            </div>
        </div>
    </div> {{!-- /wrapper

	{{!-- Notices
	{{#if alerts}}
		<div class="alert">
			<div class="wrapper">
				<div class="col-wrap">
					<div class="col">
						<div class="show-hide js-show-hide">
							<div class="js-show-hide__title">
								<div class="alert--warning__icon">!</div>

								<h2 class="show-hide__title alert--warning__title inline-block">
									Notice{{#if_ne alerts.size 1}}s{{/if_ne}}
								</h2>
							</div>

							<div class="js-show-hide__content">
								{{> partials/notice}}
							</div>

						</div>
					</div>
				</div>
			</div>
		</div>
	{{/if}}

    <div class="meta-wrap">
        <div class="wrapper">

            {{!-- Meta info
            <div class="col-wrap">

                {{#each breadcrumb}}
                    {{#if @last}}
                        {{#resolve uri assign="parent"}}
                            {{!-- Contact & NS logo
                            <p class="col col--md-12 col--lg-15 meta__item">
                                {{#if parent.description.nationalStatistic}}
                                    <img class="meta__image" src="/node_modules/ONS-Pattern-Library/dist/img/national-statistics.png" alt="National Statistics logo" />
                                {{/if}}
                                <span>{{labels.meta-contact}}: </span><br/><a href="mailto:{{parent.description.contact.email}}">{{#if parent.description.contact.name}}{{parent.description.contact.name}}{{else}}{{parent.description.contact.email}}{{/if}}</a>
                            </p>

                            {{!-- Release date
                            <p class="col col--md-12 col--lg-15 meta__item">
                                <span>{{labels.release-date}}: </span><br/>{{df parent.description.releaseDate}}<br/>
                            </p>

                            {{!-- Next release -
                            <p class="col col--md-12 col--lg-15 meta__item">
                                <span>{{labels.release-next}}: </span><br/>{{#if parent.description.nextRelease}}{{parent.description.nextRelease}}{{else}}To be announced{{/if}}
                            </p>

                            {{!-- Dataset ID
                            {{#if parent.description.datasetId}}
                                <p class="col col--md-12 col--lg-15 meta__item">
                                    <span>{{labels.dataset-id}}: </span>{{parent.description.datasetId}}&nbsp;
                                    <span><a href="#" class="tooltip-npl" title="This series ID is a unique random identifier for this individual time series. The digits themselves do not have any specific meaning" aria-label="This series ID is a unique random identifier for this individual time series. The digits themselves do not have any specific meaning"> {{labels.dataset-what-is}} </a></span>
                                </p>
                            {{/if}}
                        {{/resolve}}
                    {{/if}}
                {{/each}}
            </div>
        </div> {{!-- /wrapper
    </div> {{!-- /meta-wrap
</div> {{!-- /page-intro --}}

{{#partial "block-breadcrumb"}}
    {{> list/partials/list-page-breadcrumb label=description.edition}}
{{/partial}}


{{!-- Prevent alerts appearing --}}
{{#partial "block-alert"}}
    
{{/partial}}

{{#partial "block-intro"}}

    {{!-- Resolve title from parent --}}

    {{#each breadcrumb}}
        {{#if @last}}
            {{#resolve uri}}

                <h1 class="page-intro__title {{#if_any description._abstract description.summary}}{{else}}margin-bottom-sm--4 margin-bottom-md--4{{/if_any}}">{{#if typeLabel}}<span class="page-intro__type">{{typeLabel}}:</span>{{/if}}{{description.title}}

            {{/resolve}}
        {{/if}}
    {{/each}}

    {{!-- Output edition --}}

    {{!-- {{#if description.edition}}: {{description.edition}}{{/if}} --}}</h1>

    {{!-- Resolve summary from parent --}}

    {{#each breadcrumb}}
        {{#if @last}}
            {{#resolve uri}}
                {{#if description.summary}}
                    <p class="page-intro__content">{{description.summary}}</p>
                {{/if}}
            {{/resolve}}
        {{/if}}
    {{/each}}



{{/partial}}

{{#partial "block-metadata"}}

    <div class="meta-wrap">
        <div class="wrapper">
            <div class="col-wrap">

                {{#each breadcrumb}}
                    {{#if @last}}
                        {{#resolve uri}}
                        {{!-- Contact & NS logo--}}
                            {{> content/partials/metadata/contact class="col--md-12 col--lg-15" }}

                            {{!-- Release date --}}
                            {{> content/partials/metadata/release-date class="col--md-12 col--lg-15"}}

                            {{!-- Next release --}}
                            {{> content/partials/metadata/next-release class="col--md-12 col--lg-15"}}

                            {{!-- Dataset ID --}}
                            {{#if description.datasetId}}
                                <p class="col col--md-12 col--lg-15 meta__item">
                                    <span>{{labels.dataset-id}}: </span>{{description.datasetId}}&nbsp;
                                    <span><a href="#" class="tooltip-npl print--hide"
                                             title="This series ID is a unique random identifier for this individual time series. The digits themselves do not have any specific meaning"
                                             aria-label="This series ID is a unique random identifier for this individual time series. The digits themselves do not have any specific meaning"> {{labels.dataset-what-is}}
                                         </a></span>
                                </p>
                            {{/if}}
                        {{/resolve}}
                    {{/if}}
                {{/each}}
            </div>
        </div>
    </div>

{{/partial}}


{{#partial "block-content"}}
<div class="page-content">
    <div class="wrapper">
        <div class="col-wrap">
            <div class="col col--lg-one col--md-two-one">

                <div class="section__content--static-markdown">

                    <p class="print--hide">
                        {{#each breadcrumb}}
                            {{#if @last}}
                                <a class="underline-link" href="{{uri}}"><span class="icon icon-return--dark-small margin-right-sm--1 margin-right-md--1"></span>Return to '{{description.title}}'</a>
                            {{/if}}
                        {{/each}}
                    </p>

                    <p>Provides files to download data as it existed for this dataset on previous dates. </p>

                    <p>Statistics are most often revised for 1 of 2 reasons:</p>

                    <ol>
                        <li>For certain statistics initial estimates are released with the expectation that these may be revised and updated as further data becomes available.</li>
                        <li>Revisions may also be made when methods or systems are changed.</li>
                    </ol>

                    <p>These types of planned revisions should not be confused with errors in released statistics, which are genuine mistakes. Such mistakes occur rarely and, when they do happen, corrections are made in a timely manner, announced and clearly explained to users in line with the Code of Practice for Official Statistics (Principle 2, Practice 7).</p>

                </div>

                <h3 class="margin-top-md--4">Latest version</h3>

                {{#each downloads}}

                    {{#if file}}

                        <form method="get" action="/file" class="inline">
                            <button type="submit" title="Latest version" class="btn btn--primary btn--thin btn--narrow btn--small download-analytics">
                                {{fe (lowercase (concat uri "/" file))}} ({{fs (lowercase (concat uri "/" file))}})
                            </button>
                            <input type="hidden" name="uri" value="{{absolute (concat uri "/" file)}}"/>
                        </form>

                    {{/if}}

                {{/each}}

                {{#if versions}}

                    <h3 class="margin-top-md--5">Previous versions</h3>

                    <table class="table-advanced margin-bottom-sm--4 margin-bottom-md--4">
                        <thead>
                        <tr>
                            <th width="25%">Superseded files</th>
                            <th width="60%">Reason for update</th>
                            <th width="15%">Date superseded</th>
                        </tr>
                        </thead>
                        <tbody>
                        {{#loop versions reverse=true}}
                            <tr>
                                <td>

                                    {{#resolve this.uri}}

                                        {{#each downloads}}

                                            {{#if file}}

                                                <form method="get" action="/file" {{#if_ne downloads.size 1}}class="inline"{{/if_ne}}>
                                                    <button type="submit" title="Latest version" class="btn btn--primary btn--thin btn--narrow btn--small download-analytics">
                                                        {{fe (lowercase (concat uri "/" this.file))}} ({{fs (lowercase (concat uri "/" this.file))}})
                                                    </button>
                                                    <input type="hidden" name="uri" value="{{absolute (concat uri "/" this.file)}}"/>
                                                </form>

                                            {{/if}}

                                        {{/each}}

                                    {{/resolve}}

                                </td>
                                <td>
                                    {{#if correctionNotice}}

                                        <div class="show-hide show-hide--light js-show-hide">
                                            <div class="js-show-hide__title">
                                                <span class="no-js js-enhance--hide">Correction</span>
                                                <span>See correction</span>
                                            </div>

                                            <div class="js-show-hide__content">
                                                {{correctionNotice}}
                                            </div>

                                        </div>

                                    {{else}}
                                        Scheduled update/revision
                                    {{/if}}
                                </td>
                                <td>{{df updateDate outputFormat="d MMMM yyyy HH:mm"}}</td>
                            </tr>
                        {{/loop}}
                        </tbody>
                    </table>

                {{/if}}


                {{!-- INFORMATION AND NOTES --}}
                {{#each breadcrumb}}
                    {{#if @last}}
                        {{#resolve uri}}
                            {{#if section.markdown}}
                                <div class="section__content--static-markdown">
                                    <section>
                                        <h3>{{labels.dataset-notes}}</h3>
                                        {{md section.markdown}}
                                    </section>
                                </div>
                            {{/if}}
                        {{/resolve}}
                    {{/if}}
                {{/each}}

            </div>
        </div>
    </div>

</div> {{!-- /page-content --}}
{{/partial}}
{{> content/base/statistics typeLabel="Dataset(s)"}}
