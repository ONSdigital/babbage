{{!-- Disables breadcrumb and metadata on homepage --}}
{{#partial "block-breadcrumb"}} {{/partial}}
{{#partial "block-intro"}}
<div class="col col--md-47 col--lg-34">
    <h1 class="page-intro__title page-intro__title--home"><span class="page-intro__title page-intro__title--home-big">
    Welcome to </span>the Office for National Statistics
    </h1>
    <p class="page-intro__content page-intro__content--home-big">
        The UK's largest independent producer of official statistics and the recognised national statistical institute of the UK.
    </p>
</div>
<div class="tiles__a-z col col--md-47 col--lg-23 col--lg-offset-2 margin-top--triple-lg">
    <h2 class="margin-top-md--2 margin-top-sm--2 margin-left">Browse our statistics by A-Z</h2>
    <ul class="tiles__a-z-list padding-top-md--1 padding-top-sm--1">
        {{> partials/az-letter letter="A" class="tiles__a-z-list-item"}}
        {{> partials/az-letter letter="B" class="tiles__a-z-list-item"}}
        {{> partials/az-letter letter="C" class="tiles__a-z-list-item"}}
        {{> partials/az-letter letter="D" class="tiles__a-z-list-item"}}
        {{> partials/az-letter letter="E" class="tiles__a-z-list-item"}}
