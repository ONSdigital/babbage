{{#partial "block-content"}}
<div class="slate--alabaster">
    <div class="wrapper">
        <div class="tiles">
            <h2>Latest news, figures and releases</h2>
            <div class="col-wrap tiles__block tiles__block-no-mar-bottom">
                {{!-- Dynamic stastical highlights --}}
                <div class="col col--md-full-width col--lg-45">
                    <div class="clickable-wrap">
                        <div class="col col--md-23 col--lg-14 tiles__item tiles__item--image-type-fixed-46-rows">
                            {{>partials/t1/home-figure sections.0}}
                        </div>
                    </div>
                    <div class="clickable-wrap">
                        <div class="col col--md-23 col--lg-29 tiles__item tiles__item--image-type-fixed-46-rows">
                            {{>partials/t1/home-figure sections.1}}
                        </div>
                    </div>
                    <div class="clickable-wrap">
                        <div class="col col--md-15 col--lg-14 tiles__item tiles__item--image-type-fixed-46-rows">
                            {{>partials/t1/home-figure sections.2}}
                        </div>
                    </div>
                    <div class="clickable-wrap">
                        <div class="col col--md-15 col--lg-14 tiles__item tiles__item--image-type-fixed-46-rows">
                            {{>partials/t1/home-figure sections.3}}
                        </div>
                    </div>
                    <div class="clickable-wrap">
                        <div class="col col--md-15 col--lg-14 tiles__item tiles__item--image-type-fixed-46-rows">
                            {{>partials/t1/home-figure sections.4}}
                        </div>
                    </div>
                </div>
                <div class="col col--md-full-width col--lg-15">
                    <div class="clickable-wrap">
                        <div class="col col--lg-14 tiles__item tiles__item--image-type-fixed-30-rows">
                            <article>
                                {{!-- <h2 class="tiles__title tiles__title-h2--home"><a href="/releasecalendar">{{labels.homepage-calendar}}<span class="image-holder"><img src="../../img/t1-cal.png" alt=""></span></a></h2>
                                <p class="tiles__content">Find out when our statistics are released and sign-up to receive notifications.</p> --}}
                                <h2 class="tiles__title tiles__title-h2--home"><a href="/about/calendar">Release calendar<span class="image-holder"><img src="../../img/t1-cal.png" alt=""></span></a></h2>
                                <p class="tiles__content">Find out when our statistics are released and sign-up to receive notifications.</p>
                            </article>
                        </div>
                    </div>
                    <div class="clickable-wrap">
                        <div class="col col--lg-14 tiles__item tiles__item--image-type-fixed-30-rows">
                            <article>
                                <h2 class="tiles__title tiles__title-h2--home"><a class="icon--hide" href="https://www.gov.uk/government/statistics">{{labels.homepage-other-stats}}<span class="image-holder"><img src="../../img/t1-gov-uk--black.png" alt="GOV.UK logo" class="no-border"></span></a></h2>
                                <p class="tiles__content">Find official statistics produced by other government departments.</p>
                            </article>
                        </div>
                    </div>
                    <div class="clickable-wrap">
                        <div class="col col--lg-14 tiles__item tiles__item--image-type-fixed-30-rows">
                            <article>
                                <h2 class="tiles__title tiles__title-h2--home"><a href="/about/localstatistics">{{labels.local-stats-header}}<span class="image-holder"><img src="../../img/t1-local.png" alt="" class="no-border"></span></a></h2>
                                <p class="tiles__content">{{labels.local-stats-text}}</p>
                            </article>
                        </div>
                    </div>
                </div>
                <div class="tiles__block tiles__block-no-mar-top">
                    <div class="clickable-wrap">
                        <div class="col col--lg-29 tiles__item tiles__item--image-type-fixed-30-rows">
                            <article>
                                <h2 class="tiles__title tiles__title-h2--home"><a href="http://www.ons.gov.uk/ons/guide-method/census/index.html">{{labels.homepage-census}}<span class="image-holder"><img src="../../img/t1-census.png" alt="" class="no-border"></span></a></h2>
                                <p class="tiles__content">{{labels.homepage-census-sum}}</p>
                            </article>
                        </div>
                    </div>
                    <div class="clickable-wrap">
                        <div class="col col--lg-29 tiles__item tiles__item--image-type-fixed-30-rows">
                            <article>
                                <h2 class="tiles__title tiles__title-h2--home"><a href="http://visual.ons.gov.uk">{{labels.homepage-visual}}<span class="image-holder"><img src="../../img/t1-visual-ons.png" alt="Example of interactive data visualisation" class="no-border"></span></a></h2>
                                <p class="tiles__content">{{labels.homepage-visual-sum}}</p>
                            </article>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
{{/partial}}
