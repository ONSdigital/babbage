<div class="col col--md-31 col--lg-41 margin-left-md--1">
	<div class="table-of-contents js-sticky-toc print--avoid-break">
		<h2 class="table-of-contents__title">{{#if tocTitle}}{{tocTitle}}{{else}}{{labels.table-of-contents}}{{/if}}</h2>
		<ol id="toc" class="table-of-contents__list">
			{{#each sections}}
			<li class="table-of-contents__item">
				<a href="#{{slugify title}}" class="js-scroll" data-ga-event data-ga-event-category='toc-link' data-ga-event-label="{{title}}">{{title}}</a>
			</li>
			{{/each}}
			{{#each accordion}}
			<li class="table-of-contents__item">
				<a href="#{{slugify title}}" class="js-scroll expand-accordion" data-ga-event data-ga-event-category='toc-link' data-ga-event-label="{{title}}">{{title}}</a>
			</li>
			{{/each}}
			{{#if relatedMethodology}}
			<li class="table-of-contents__item">
				<a href="#methodology" class="js-scroll expand-accordion" data-ga-event data-ga-event-category='toc-link' data-ga-event-label="{{labels.methodology}}">{{labels.methodology}}</a>
			</li>
			{{/if}}
		</ol>
	</div>
</div>