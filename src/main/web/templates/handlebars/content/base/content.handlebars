{{#partial "block-main"}}
{{#if_eq type "bulletin"}}
	<div class="page-bulletins background--astral text--white">
		<div class="wrapper">
			<div class="col-wrap">
				<div class="col">
					<nav>
						<div class="breadcrumb-bulletins print--hide">
							<ol class="breadcrumb-bulletins__list">
								{{#each breadcrumb}}
									<li class="breadcrumb-bulletins__item padding-bottom--0 padding-top--0">
										<a class="breadcrumb-bulletins__link" href="{{uri}}">{{description.title}}</a>
									</li>
								{{/each}}
								<li class="breadcrumb__item padding-bottom--0 padding-top--0">
									{{description.title}}
								</li>
							</ol>
						</div>
					</nav>
					
					<div class="col margin-left--0">
						<h1 class="page-intro__title padding-bottom--0 padding-top--0 margin-top--2 font-weight-700 {{#if_any description._abstract description.summary}}margin-bottom--2 {{else}}margin-bottom--4{{/if_any}}">{{description.title}}{{#if description.edition}}: {{description.edition}}{{/if}}</h1>
						<p class="page-intro__content margin-top--2 margin-bottom--3 padding-bottom--0 padding-top--0">
						{{#if description._abstract}}
							{{description._abstract}}
						{{else}}
							{{#if description.summary}}
								{{description.summary}}
							{{/if}}
						{{/if}}</p>
					</div>
				</div> {{!-- /col --}}
			</div> {{!-- /col-wrap --}}
		</div> {{!-- /wrapper --}}
		<div class="wrapper">
			<div class="col-wrap">
				{{!-- Latest info --}}
				{{#unless (endsWith (parentPath uri) "previous")}} 
					{{!-- If not a previous version workout the latest link --}}
					{{#if description.latestRelease}}
						<div class="col bulletin-alerts bulletin-alerts__latest">
							<p class="bulletin-alerts__title">{{labels.this-is-the-latest-release}}
							<a class="bulletin-alerts__link" href="{{parentPath uri}}/previousReleases">{{labels.view-previous-releases}}</a></p>
						</div>
					{{else}}
						{{#resolve (concat (parentPath uri) "/latest") filter="title"}}
							<div class="col bulletin-alerts bulletin-alerts__not-latest">
								<p class="bulletin-alerts__title">{{labels.this-is-not-the-latest-release}}
								<a class="bulletin-alerts__link" href="{{uri}}">{{labels.view-latest-release}}</a></p>
							</div>
						{{/resolve}}
					{{/if}}
				{{else}}
					{{!-- If an older version of any release link to latest version of that release --}}
					<div class="col bulletin-alerts bulletin-alerts__not-latest">
						<p class="bulletin-alerts__title">{{labels.this-has-been-superseded}} <a class="bulletin-alerts__link" href="{{parentPath (parentPath uri)}}">{{labels.view-corrected-version}}</a></p>
					</div>
				{{/unless}}

				{{!-- Corrections & alerts --}}
				{{#if_any versions alerts}}
				{{!-- {{else}} --}}
					<div class="show-hide show-hide--dark margin-top-sm--3 margin-top-md--3 js-show-hide">
						<div class="js-show-hide__title">
							<button class="js-show-hide__button margin-top--2" type="button" aria-expanded="false" aria-controls="collapsible-0">
								<div class="bulletin-alerts--border">
									<span class="icon icon-info icon-info--info-box"></span>

									<h2 class="show-hide__title bulletin-alerts--warning__title">
										{{!-- Lots of 'if's to handle plurals and combinations of corrections or notices --}}
										{{#if_all versions alerts}}
											{{labels.corrections-and-notices}}
										{{else}}
											{{#if versions}}
												{{#if_ne versions.size "1"}}{{labels.correction}}{{else}}{{labels.corrections}}{{/if_ne}}
											{{else}}
												{{#if_ne alerts.size "1"}}{{labels.notice}}{{else}}{{labels.notices}}{{/if_ne}}
											{{/if}}
										{{/if_all}}
									</h2>
								</div>
							</button>
						</div>

						<div class="js-show-hide__content">
							{{> partials/correction}}
							{{> partials/notice}}
						</div>

					</div>
					{{!-- {{#block "block-alert"}}
						{{#if no-versions}}
							{{> partials/alert no-versions=true}}
						{{else}}
							{{> partials/alert}}
						{{/if}}
					{{/block}} --}}
				{{/if_any}}
				{{#partial "block-intro-secondary"}}
					{{!-- Latest release flag, corrections and notices --}}
					{{#block "block-alert"}}
						{{#if no-versions}}
							{{> partials/alert no-versions=true}}
						{{else}}
							{{> partials/alert}}
						{{/if}}
					{{/block}}
				{{/partial}}
			</div>
		</div>
		<div class="wrapper">
			<div class="col-wrap margin-top--3 margin-left--0">
					{{#if description.nationalStatistic}} <div class="col col--md-4 col--lg-4"><a
					href="https://www.statisticsauthority.gov.uk/national-statistician/types-of-official-statistics/"><img
					class="meta__image" src="/img/national-statistics.png"
					alt="This is an accredited national statistic."/></a></div>{{/if}}
				<p class="col {{class}} col--md-12 col--lg-15 margin-bottom-sm--1 margin-top--0 padding-bottom--0 padding-top--0">
					<span class="font-weight-700">{{labels.contact}}: </span><br/><a href="mailto:{{description.contact.email}}" class="text--white"><span class="visuallyhidden">Email </span>{{#if description.contact.name}}{{description.contact.name}}{{else}}{{description.contact.email}}{{/if}}</a>
				</p>

				<p class="col {{class}} col--md-one-third col--lg-one-third margin-bottom-sm--1 margin-top--0 padding-bottom--0 padding-top--0">
					<span class="font-weight-700">{{labels.release-date}}: </span><br/>{{df description.releaseDate}}<br/>
					{{#if_all (eq type "timeseries") versions}}{{#unless previous}}<a href="{{uri}}/previous">View previous versions</a>{{/unless}}{{/if_all}}
				</p>

				<p class="col {{class}} col--md-one-third col--lg-one-third margin-bottom-sm--1 margin-top--0 padding-bottom--0 padding-top--0">
					<span class="font-weight-700">{{labels.next-release}}: </span><br/>{{#if description.nextRelease}}{{description.nextRelease}}{{else}}To be announced{{/if}}
				</p>
			</div>
		</div>
	</div> {{!-- /page-intro --}}
{{else}}
    <div class="page-neutral-intro {{#if_eq type "home_page"}}background--astral{{else}}background--gallery{{/if_eq}}">
	<div class="wrapper">
		<div class="col-wrap">
			<div class="col">
				{{#block "block-breadcrumb"}}
                    {{#if_eq type "product_page"}}
                        {{> partials/breadcrumb-neutral}}
                    {{else}}
                        {{> partials/breadcrumb}}
                    {{/if_eq}}

				{{/block}}
				
				{{#block "block-intro"}}
				<div class="col col--md-47 col--lg-48">
                    {{#if_eq type "product_page"}}
                        {{> content/partials/intro-neutral}}
                    {{else}}
                        {{> content/partials/intro}}
                    {{/if_eq}}
				</div>
				{{/block}}
			</div> {{!-- /col --}}
		</div> {{!-- /col-wrap --}}
	</div> {{!-- /wrapper --}}

	{{#block "block-intro-secondary"}}{{/block}}
</div> {{!-- /page-intro --}}
{{/if_eq}}
<div class="page-content">
	{{#block "block-content"}}{{/block}}
</div>
{{/partial}}
{{!-- Inherits base template structure --}}
{{> base/base}}