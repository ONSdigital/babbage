<main id="main" role="main" tabindex="-1">
<div class="page-header page-header--article page-header--stat-bulletin slate--blue">
	<div class="wrapper" >
		{{> partials/breadcrumb}}
		<p class="page-header__title page-header--stat-bulletin-desc">
			Statistical bulletin
		</p>
		<h1 class="page-header__title flush--half--bottom">{{description.title}}{{#if description.edition}}: {{description.edition}}{{/if}}</h1>
		<div class="grid-wrap mini">
			<p class="grid-col tablet-grid-one-quarter {{#if description.nationalStatistic}}page-header__meta-block-with-img{{/if}}">
				{{#if description.nationalStatistic}}
				<img src="/node_modules/ONS-Pattern-Library/dist/img/national-statistics.png" alt="National Statistics logo" />
				{{/if}}
				<span>{{labels.meta-contact}}: </span><br/><a href="mailto:{{description.contact.email}}">{{#if description.contact.name}}{{description.contact.name}}{{else}}{{description.contact.email}}{{/if}}</a>
			</p>
			<p class="grid-col tablet-grid-one-quarter divider--border-left">
				<span>{{labels.release-date}}: </span><br/>{{df description.releaseDate}}<br/>
				{{#resolve (concat (parentPath uri) "/latest") filter="title"}}
				{{#if_eq uri ../uri}}
				<a class="print-hidden" href="{{parentPath uri}}/previousReleases">{{labels.release-prev}}</a>
				{{else}}
				<a class="print-hidden" href="{{uri}}">{{labels.release-next}}</a>
				{{/if_eq}}
				{{/resolve}}
			</p>
			{{#if description.nextRelease}}
			<p class="grid-col tablet-grid-one-quarter divider--border-left">
				<span>{{labels.release-next}}: </span><br/>{{description.nextRelease}}
			</p>
			{{/if}}
			<p class="grid-col tablet-grid-one-quarter divider--border-left print-hidden">
				<a href="" id="jsEnhancePrint" class="link-complex nojs-hidden js-enhance--show">
					<span class="icon-print"></span>
					<span class="link-complex__target print-analytics">{{labels.print-report}}</span>
				</a>
				<br class="nojs-hidden js-enhance--show"/>
				<a href="{{uri}}/pdf" class="link-complex">
					<span class="icon-download" role="presentation"></span>
					<span class="link-complex__target">{{labels.dl-pdf}}</span>
				</a>
			</form>
		</p>
	</div>
</div>
</div>
<!-- Table of contents -->
<div class="wrapper">
<p class="lede">{{description.summary}}</p>
<div class="panel--separated--bottom page-break-avoid">
	<h2>{{labels.toc}}</h2>
	<ol id="toc" class="table-of-contents-ordered-list">
		{{#each sections}}
		<li>
			<a href="#{{slugify title}}" class="jsEnhanceAnimateScroll">{{title}}</a>
		</li>
		{{/each}}
		{{#if accordion}}
		{{#each accordion}}
		<li>
			<a href="#{{slugify title}}" class="jsEnhanceAnimateScroll expand-accordion">{{title}}</a>
		</li>
		{{/each}}
		{{/if}}
	</ol>
</div>
</div>
<!-- Body Sections -->
<div class="wrapper wrapper--content panel--bottom-mar print-links">
<div class="grid-wrap">
	<article class="grid-col desktop-grid-two-thirds tablet-grid-full-width">
		{{#each sections}}
		{{!-- Prevent bottom border on last section before accordian --}}
		{{#if @last}}
		<section id="{{slugify title}}" class="panel--pad">
			{{else}}
			<section id="{{slugify title}}" class="panel--separated--bottom panel--pad">
				{{/if}}
				<header>
					<h2>{{@index_1}}. {{title}}</h2>
				</header>
				{{md markdown}}
				<a class="print-hidden jsEnhanceAnimateScroll" href="#toc" title="{{labels.back-to-toc}}">{{labels.back-to-toc}}</a>
			</section>
			{{/each}}
			<!-- Accordion -->
			{{#if accordion}}
			<div class="panel--mar">
				{{#each accordion}}
				<section id="{{slugify title}}" class="accordion is-collapsed">
					<h2 class="accordion__title" aria-controls="content-{{@index}}">{{title}}</h2>
					<div class="accordion__content panel--bottom-mar-small" aria-labelledby="sc-{{@index}}">
						{{md markdown}}
						<a class="print-hidden jsEnhanceAnimateScroll" href="#toc" title="{{labels.back-to-toc}}" class="">{{labels.back-to-toc}}</a>
					</div>
				</section>
				{{/each}}
			</div>
			{{/if}}
		</article>
		<!-- Sidebar -->
		<div class="grid-col desktop-grid-one-third tablet-grid-full-width panel--top-mar">
			<!-- article__content -->
			{{#if relatedData}}
			<div class="box box--green box--green--separated--left">
				<div class="box__inner border box--padded">
					<header class="box__header">
						<h3>{{labels.bulletin-related-data}}</h3>
					</header>
					<div class="box__content">
						<ul class="list--neutral">
							{{#each relatedData}}
							{{#resolve this.uri filter="title"}}
							<li class="flush--third--bottom"><a href="{{absolute uri}}">{{title}}</a></li>
							{{/resolve}}
							{{/each}}
						</ul>
					</div>
				</div>
			</div>
			{{/if}}
			<!-- /article__content -->
			<div class="box__inner border box--padded">
				<h3 class="box__section__header">
				{{labels.bulletin-contact}}
				</h3>
				<!-- /box__header -->
				<div class="box__content">
					<address>
						{{#if description.contact.name}}{{description.contact.name}}<br/>{{/if}}
						<a href="mailto:{{description.contact.email}}">{{description.contact.email}}</a></br>
						{{#if description.contact.telephone}}{{labels.phone}}: {{description.contact.telephone}}{{/if}}
					</address>
				</div>
				<!-- /box__content -->
			</div>
		</div>
	</div>
</div>
<!-- panel -->
<div class="slate--mid-grey">
	<!-- wrapper -->
	<div class="wrapper panel--top-pad print-links">
		<!-- grid-wrap -->
		<div class="grid-wrap">
			<div class="grid-col desktop-grid-two-thirds tablet-grid-full-width">
				{{#if relatedBulletins}}
				<article class="box box--teal--separated--left">
					<div class="box__inner border box--padded solid">
						<h3>{{labels.bulletin-related-bulletins}}</h3>
						<ul class="list--neutral">
							{{#each relatedBulletins}}
							{{#resolve this.uri filter="description"}}
							<li>
								<h3 class="flush--bottom">
								<a href="{{uri}}">{{description.title}}{{#if description.edition}}: {{description.edition}}{{/if}}</a>
								</h3>
								<p>{{md description.summary}}</p>
							</li>
							{{/resolve}}
							{{/each}}
						</ul>
					</div>
				</article>
				{{/if}}
			</div>
			<div class="grid-col desktop-grid-one-third tablet-grid-full-width">
				{{> partials/local-box}}
			</div>
		</div>
	</div>
</div>
</main>